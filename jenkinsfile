pipeline {
    agent any

    stages {
        stage('maven build') {
          when{
            branch"Develop"
           }
            steps {
        echo "Maven build..."
              }
        }

     stage('dev deploy') {
            when{
            branch"Develop"
           }
             steps {
              echo "Deploying to dev"
               }
        }   
          stage('test deploy') {
            when{
            branch"test"
           }
             steps {
              echo "Deploying to test"
               }
        }

         stage('production deploy') {
            when{
            branch"production"
           }
             steps {
              echo "Deploying to production  "
               }
        }

  



    }
}
